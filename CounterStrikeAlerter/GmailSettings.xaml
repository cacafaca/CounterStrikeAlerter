<Window x:Class="CounterStrikeAlerter.GmailSettings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CounterStrikeAlerter"
        mc:Ignorable="d"
        Title="GmailSettings" Height="159.292" Width="300" SizeToContent="Height">
    <Grid>
        <StackPanel Margin="3">
            <Label x:Name="label" Content="email address"/>
            <TextBox x:Name="userTextBox" Height="23" TextWrapping="Wrap" Text="TextBox"/>
            <Label x:Name="label1" Content="password"/>
            <TextBox x:Name="passwordTextBox" Height="23" TextWrapping="Wrap" Text="TextBox"/>
            <Button x:Name="saveButton" Content="Save" Click="saveButton_Click"/>
        </StackPanel>

    </Grid>
</Window>
